    "css-loader": "^5.0.0",
    "eslint": "5.13.0",
    "eslint-config-htmlacademy": "0.5.2",
    "eslint-plugin-react": "7.18.3",
    "file-loader": "^6.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "25.1.0",
    "react-test-renderer": "16.12.0",
    "redux-devtools-extension": "2.13.8",
    "redux-mock-store": "1.5.4",
    "style-loader": "^2.0.0",
    "webpack": "4.41.6",
